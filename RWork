Candidates <- c("Jeb","Donald","Ted","Marco","Carly","Hillary","Bernie")
ABC <- c(4, 62, 51, 21, 2, 14, 15)
CBS <- c(12, 75, 43, 19, 1, 21, 19)
DF2016 <- data.frame(Candidates,ABC,CBS)
print(DF2016)
  Candidates ABC CBS
1        Jeb   4  12
2     Donald  62  75
3        Ted  51  43
4      Marco  21  19
5      Carly   2   1
6    Hillary  14  21
7     Bernie  15  19
meanABC <- mean(DF2016[,2])
meanCBS <- mean(DF2016[,3])
meanABC
[1] 24.14286
meanCBS
[1] 27.14286
